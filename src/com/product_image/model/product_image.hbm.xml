<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class name="com.product_image.model.Product_ImageVO" table="product_image">
        
        <id name="pro_img_no" column="pro_img_no">
            <generator class="sequence">
                <param name="sequence_name">product_image_seq</param>
            </generator> 
        </id>
               
         <property name="img" column="img"/>
         <property name="img_nam" column="img_nam"/>
    
        <many-to-one name="productVO" class="com.product.model.ProductVO" lazy="false">
            <column name="pro_no"/>
        </many-to-one>

    </class>

</hibernate-mapping>